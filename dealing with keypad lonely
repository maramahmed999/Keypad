#include <Keypad.h>

const byte Rows=4;
const byte Colms=4;
char keys[Rows][Colms]={
 {'1','2','3','A'}
,{'4','5','6','B'}
,{'7','8','9','C',}
  ,{'*','0','#','D'}};


byte rowpins[Rows] ={5,4,3,2};
byte colpins[Colms] ={9,8,7,6};
Keypad keypad = Keypad(makeKeymap(keys),rowpins,colpins,Rows,Colms);

void setup()
{
Serial.begin(9600);
}
void loop()
{
  char key = keypad.getKey();

  if (key){ 

  Serial.println(key);
}
  
}
